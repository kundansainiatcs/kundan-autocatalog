#%RAML 1.0
title: kundan-demo-api
description: Send sms dynamically test demo with Venkat.
version: 1.0.0
mediaType: 
- application/json
baseUri: api/sms/{version}
  
types:
  email:
    properties:
      toPhone: string
      messageBody: string
      fromPhone: string
      messageSubject: string
      
      
  error:
    properties:
      errorcode?: string
      errormessage?: string
      errordescription?: string
/sms:
  
  post:
    description: Send Email Again
    body:
      application/json:
        type: email
        example:
          
    responses:
      201:
        body:
          application/json:
            example:
              { 
                "message": "SMS Sent"
              }
